import util.assert
import math

assert_equal sin(0.0), 0.0
assert_equal cos(0.0), 1.0
assert_equal tan(0.0), 0.0
assert_equal atan(0.0), 0.0
assert_equal asin(0.0), 0.0
assert_equal acos(1.0), 0.0
assert_equal exp(0.0), 1.0
assert_equal log(1.0), 0.0

let c0_0 = Complex(0.0, 0.0)
let c1_0 = Complex(1.0, 0.0)

assert_equal sin(c0_0), c0_0
assert_equal cos(c0_0), c1_0
assert_equal tan(c0_0), c0_0
assert_equal atan(c0_0), c0_0
assert_equal asin(c0_0), c0_0
assert_equal acos(c1_0), c0_0
assert_equal exp(c0_0), c1_0
assert_equal log(c1_0), c0_0

